{
  "name": "tsoa-project",
  "version": "1.0.0",
  "main": "./build/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "routes": "tsoa spec-and-routes",
    "cp": "copy -r ./public ./build",
    "build": "tsoa spec-and-routes && tsc --build .",
    "debug": "node --inspect ./build/index.js",
    "start": "node ./build/index.js",
    "clean": "rm -rf ./build && rm -rf ./node_modules"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "fs-extra": "^10.1.0",
    "multer": "^1.4.5-lts.1",
    "pdf2html": "^2.6.7",
    "swagger-ui-express": "^4.4.0",
    "tsoa": "^3.14.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.23",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3"
  }
}
